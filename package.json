{
  "name": "solid-cockpit",
  "description": "Solid-JS & Cockpit-CMS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "concurrently 'del-cli node_modules/.cache' 'del-cli dist'",
    "lint:types": "tsc --noEmit",
    "lint:script": "eslint --ext .tsx,.ts,.jsx,.js src/",
    "lint": "concurrently 'yarn:lint:*'",
    "dev": "poi src/index.html --serve --no-clear-console",
    "build": "yarn clean && poi src/index.html --prod --bundle-report --no-clear-console"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "date-fns": "^2.4.1",
    "destyle": "https://github.com/r0skar/destyle.css",
    "gsap": "^2.1.3",
    "headful": "^1.0.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "p-cancelable": "^2.0.0",
    "polished": "^3.4.1",
    "solid-js": "^0.11.1",
    "typestyle": "^2.0.4",
    "universal-router": "^8.3.0",
    "wretch": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@poi/plugin-bundle-report": "^12.1.1",
    "@r0skar/eslint-config": "https://github.com/r0skar/eslint-config",
    "@types/gsap": "^1.20.2",
    "@types/history": "^4.7.3",
    "@types/lodash": "^4.14.141",
    "@types/nprogress": "^0.2.0",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-solid": "^0.1.1",
    "concurrently": "^4.1.2",
    "del-cli": "^3.0.0",
    "eslint": "^6.5.1",
    "fast-async": "7",
    "poi": "^12.7.2",
    "raw-loader": "^3.1.0",
    "typescript": "beta"
  }
}
